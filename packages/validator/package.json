{
  "name": "@cosmo/validator",
  "version": "2.0.0",
  "description": "Runtime validation and sanitization for Cosmo UI components - ensures safe AI-generated UI",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "cosmo",
    "ui",
    "validator",
    "ai-first",
    "sanitization",
    "runtime-validation",
    "type-safe"
  ],
  "author": "Antonio Consales",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/antonioconsales/cosmo-ui.git",
    "directory": "packages/validator"
  },
  "homepage": "https://github.com/antonioconsales/cosmo-ui/tree/main/packages/validator#readme",
  "bugs": {
    "url": "https://github.com/antonioconsales/cosmo-ui/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@cosmo/core-schema": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^2.1.0"
  }
}
