{
  "name": "@cosmo/ai-adapter",
  "version": "2.0.0",
  "description": "AI integration layer for Cosmo UI - prompts, Spatial Intent, Adaptive Complexity, Voice-First, and Multi-Agent coordination",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cosmo",
    "ui",
    "ai",
    "adapter",
    "llm",
    "prompt",
    "spatial-intent",
    "adaptive-complexity",
    "voice-first",
    "multi-agent",
    "openai",
    "anthropic",
    "gemini"
  ],
  "author": "Antonio Consales",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/antonioconsales/cosmo-ui.git",
    "directory": "packages/ai-adapter"
  },
  "homepage": "https://github.com/antonioconsales/cosmo-ui/tree/main/packages/ai-adapter#readme",
  "bugs": {
    "url": "https://github.com/antonioconsales/cosmo-ui/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@cosmo/core-schema": "workspace:*",
    "@cosmo/validator": "workspace:*",
    "@toon-format/toon": "^2.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
